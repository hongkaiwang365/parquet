{
    "name": "hongkaiwang365/parquet",
    "description": "parquet file For php7.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hongkaiwang365",
            "email": "529988248@qq.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-gmp" : "*",
        "ext-bcmath" : "*",
        "ext-zlib": "*",
        "pear/math_biginteger" : "^1.0",
        "packaged/thrift": "^0.13.0",
        "nelexa/buffer": "^1.3"
    },
    "suggest" : {
      "ext-snappy" : "Install/compile snappy extension to get support for snappy compression reading/writing"
    },
    "autoload": {
        "psr-4": {
            "hongkaiwang365\\parquet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hongkaiwang365\\parquet\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpunit/php-code-coverage": "^7.0"
    }
}
